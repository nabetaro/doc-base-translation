#. type: =item
#: ../../install-docs.in:343
msgid "B<-h>, B<--help>"
msgstr "B<-h>, B<--help>"

# type: textblock
#. type: textblock
#: ../../install-docs.in:345
msgid "Show a short help message."
msgstr "Afficher un court message d'aide."

# type: =item
#. type: =item
#: ../../install-docs.in:354
msgid "B<-V>, B<--version>"
msgstr "B<-V>, B<--version>"

# type: textblock
#. type: textblock
#: ../../install-docs.in:356
msgid "Display version information."
msgstr "Afficher les informations de version."

